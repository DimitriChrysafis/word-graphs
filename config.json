    "physics": {
        "enabled": true,
        "barnesHut": {
            "gravitationalConstant": -20000,
            "springLength": 100,
            "springConstant": 0.05,
            "damping": 0.4
        },
        "minVelocity": 0.75,
        "stabilization": {
            "enabled": true,
            "iterations": 200
        }
    },
    "interaction": {
        "zoomView": true,
        "selectable": true,
        "selectConnectedEdges": true,
        "hover": true,
        "keyboard": {
            "enabled": true,
            "bindToWindow": false
        }
    },
    "clustering": {
        "enabled": true,
        "clusterEdgeThreshold": 150,
        "clusterNodeProperties": {
            "borderWidth": 3,
            "color": {
                "background": "#FFD700",
                "border": "#FFA500"
            },
            "size": 50
        }
    }
