{
    "nodes": {
        "borderWidth": 1,
        "borderWidthSelected": 2,
        "shape": "dot",
        "size": 10,
        "color": {
            "background": "#f1c40f",
            "border": "#e67e22"
        },
        "font": {
            "size": 14,
            "color": "#2c3e50",
            "face": "arial"
        }
    },
    "edges": {
        "width": 2,
        "color": {
            "color": "#7f8c8d",
            "highlight": "#e74c3c"
        },
